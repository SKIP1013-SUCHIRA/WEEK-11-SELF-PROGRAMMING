package selfProgramming;

public class OneDimensionTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int [] test = new int[5];
		
		test[0] = 85;
		test[1] = 98;
		test[2] = 75;
		test[3] = 87;
		test[4] = 68;
		
		for(int i=0; i<test.length; i++)
			System.out.println("The test for" +" index "+i+" = "+ test[i]);
			

	}

}
